{
  "name": "@aou-http/server",
  "version": "0.0.7",
  "main": "index.js",
  "types": "index.d.ts",
  "repository": {
    "url": "https://github.com/lzldev/aou-http"
  },
  "napi": {
    "name": "aou",
    "triples": {
      "defaults": true,
      "additional": [
        "aarch64-apple-darwin",
        "x86_64-unknown-linux-musl"
      ]
    }
  },
  "license": "MIT",
  "devDependencies": {
    "@napi-rs/cli": "^2.18.2",
    "ava": "^6.0.1"
  },
  "ava": {
    "timeout": "3m"
  },
  "engines": {
    "node": ">= 10"
  },
  "files": [
    "LICENSE",
    "README.md",
    "package.json",
    "index.js",
    "index.d.ts"
  ],
  "scripts": {
    "artifacts": "napi artifacts",
    "build:local": "napi build --platform && yarn types:extend",
    "build:local:release": "napi build --platform --release && yarn types:extend",
    "build": "napi build --platform --release",
    "build:debug": "napi build --platform",
    "prepublishOnly": "napi prepublish -t npm",
    "test": "ava",
    "universal": "napi universal",
    "version": "napi version",
    "types:extend": "cat ./extend.d.ts >> ./index.d.ts && cat ./extend.js >> ./index.js"
  }
}
